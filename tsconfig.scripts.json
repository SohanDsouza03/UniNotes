        "target": "ES2022",
        "module": "CommonJS",
        "rootDir": "./scripts",
        "outDir": "./dist-scripts",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["scripts/**/*.ts"]
}